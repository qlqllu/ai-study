   "source": [
    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1 / (1 + np.exp(-x))\n",
    "\n",
    "def sigmoid_derivative(x):\n",
    "    return sigmoid(x) * (1 - sigmoid(x))\n",
    "\n",
    "print(sigmoid(np.array([1,2,3])))\n",
    "print(sigmoid_derivative(np.array([1,2,3])))"
   ]
